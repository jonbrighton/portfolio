{
  "env": {
    "browser": true,
    "commonjs": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "globals": {
    "process": true,
    "__dirname": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": 0,
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "no-unused-vars": "off",
    "no-console": "off"
  }
}
